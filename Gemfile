source 'http://rubygems.org'

group :development do
  gem 'activerecord' ## not needed if you're just using KeyBuilder
  gem 'activesupport' ## KeyBuilder relys on several helper methods, can be re-factored to not need this
  gem 'rake'
  gem 'jeweler'
end

group :test do
  gem 'sqlite3'
  gem 'rspec', '~> 2.5'
end
